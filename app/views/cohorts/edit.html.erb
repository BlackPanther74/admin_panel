<h1>Cohorts#edit</h1>
<p>Find me in app/views/cohorts/edit.html.erb</p>

<%= form_with(model: @course) do |form| %>
        <label>Name:</label> <%= form.text_field(:name) %><br />
        <label>Start Date:</label> <%= form.text_field(:start_date) %><br />
        <label>End Date:</label> <%= form.text_field(:end_date) %><br />
        <label>Class Size:</label> <%= form.text_field(:class_size) %><br />
        <label>Course:</label> <%= form.text_field(:course) %><br /><br />
        <%= form.submit %>
      <% end %>