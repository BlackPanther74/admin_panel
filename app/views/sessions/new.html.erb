<h1>Sign In</h1>
<hr>

<div class="row">
  <div class="col-sm-4">
  </div>


  <div class="col-sm-4">
    <div class="card">
      <div class="card-body">
<%= form_with scope: 'session', method: :post, url: login_path do |f| %>
  <%= f.label :email%> <br>
  <%= f.text_field :email %> <br>
  <%= f.label :password%> <br>
  <%= f.password_field  :password %> <br> <br>
  <%= f.submit  class: "btn btn-primary", value: "Log In"%> <br>
<% end %>
      </div>
    </div>
  </div>
</div>
<hr>
