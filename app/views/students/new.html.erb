<h1>Students#new</h1>
<p>Find me in app/views/students/new.html.erb</p>

<%# <div class="form-group"> %>
<%= form_with(model: @student) do |form| %>
<div class="form-group">
  First Name: <%= form.text_field(:first_name) %>
</div>
<div class="form-group">
  Last Name: <%= form.text_field(:last_name) %> <br />
</div>
<div class="form-group">
  <label>Age:</label> <br> 
  <%= form.text_field(:age) %><br />
</div>
<div class="form-group">
  <label>Education:</label> <br>
  <%= form.text_field(:education) %><br /><br />
</div>
<div class="btn btn-primary">
  <%= form.submit class: "btn btn-primary" %>
</div>
<% end %>
</div>

