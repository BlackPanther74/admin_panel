<h1>Edit Student</h1>

<hr>
<div class="row">

  <div class="col-sm-4">
  </div>


  <div class="col-sm-4">
    <div class="card">
      <div class="card-body">
        <%= form_with(model: @student) do |form| %>
          <label>First Name:</label> <br>
          <%= form.text_field(:first_name) %><br />
          <label>Last Name:</label> <br>
          <%= form.text_field(:last_name) %><br />
          <label>Age:</label> <br>
          <%= form.text_field(:age) %><br />
          <label>Education:</label> <br>
          <%= form.text_field(:education) %><br /><br />
          <%= form.submit class: "btn btn-primary"%>
        <% end %>
      </div>
    </div>
  </div>
</div>
<hr>