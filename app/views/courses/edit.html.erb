<h1>Edit Course</h1>
<hr>

      <%= form_with(model: @course) do |form| %>
        <label>Name:</label> <br>
        <%= form.text_field(:name) %><br />
        <label>Hours:</label> <br>
        <%= form.text_field(:hours) %><br /><br />
        <%= form.submit class: "btn btn-primary" %>
      <% end %>